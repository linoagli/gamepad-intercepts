<UserControl x:Class="GamePad_Intercepts_Keyboard.WPF.Key"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GamePad_Intercepts_Keyboard"
             mc:Ignorable="d" 
             d:DesignHeight="60" d:DesignWidth="60">
    <Grid x:Name="grid_background" Background="#FF0C060F">
        <Label x:Name="label_character" Content="*" Margin="5" Foreground="#FFD3CFCF" FontSize="14" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
    </Grid>
</UserControl>
